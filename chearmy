#include <iostream>

using namespace std;

int main(){
	int t;
	int oddDigits,temp;
	int value, chk, ans;
	
	cin>>t;
	chk = 0;
	ans = 0;
	
	cin>>value;
	while(chk < value){
			temp = ans;
			
			oddDigits = 0;
			while(temp > 0){
			
				
				if(temp % 2 == 1){
					oddDigits++;
					break;
				}
				temp = temp/10;
			}
			
			
		if(oddDigits == 0)
			chk++;
		ans++;
	}
		
	ans--;
	cout<<ans<<endl;
	t--;
	
	while(t--){
		cin>>value;
		
		if(value < chk){
		
			chk = 0;
			ans = 0;
		}
		else
			ans++;
	
		
		
		while(chk < value){
			temp = ans;
			
			oddDigits = 0;
			while(temp > 0){
			
				
				if(temp % 2 == 1){
					oddDigits++;
					break;
				}
				temp = temp/10;
			}
			
			
			if(oddDigits == 0)
				chk++;
			ans++;
		}
		
		ans--;
		cout<<ans<<endl;
	}
	return 0;
}

#include <iostream>

using namespace std;

int main(){
	int t;
	int oddDigits,temp, beforeOdd;
	int value, chk, ans;
	
	cin>>t;
	
	while(t--){
	chk = 0;
	ans = 0;
	
	cin>>value;
	while(chk < value){
			temp = ans;
			beforeOdd= 0;
			oddDigits = 0;
			while(temp > 0){
			
				
				if(temp % 2 == 1){
					oddDigits++;
					break;
				}
				temp = temp/10;
				beforeOdd++;
			}
			
			
		if(oddDigits == 0){
			chk++;
			ans++;
		}
		else{
			if(beforeOdd == 0)
				ans++;
			else 
				ans = ans + beforeOdd*10;
		}
	}
		
	ans--;
	cout<<ans<<endl;
	}
	
	
	/*while(t--){
		cin>>value;
		
		/*if(value < chk){
		
			chk = 0;
			ans = 0;
		}
		else
			ans++;
		ans = 0;
		chk = 0;
	
		
		
		while(chk < value){
			temp = ans;
			beforeOdd= 0;
			oddDigits = 0;
			while(temp > 0){
			
				
				if(temp % 2 == 1){
					oddDigits++;
					break;
				}
				temp = temp/10;
				beforeOdd++;
			}
			
			
		if(oddDigits == 0){
			chk++;
			ans++;
		}
		else
			ans = ans + beforeOdd*10;
	}
		
			ans--;
			cout<<ans<<endl;
		
	}*/
	return 0;
}

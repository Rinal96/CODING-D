#include <iostream>

using namespace std;

int main(){
	int t;
	int oddDigits,dig,temp;
	int value, chk, ans;
	
	cin>>t;
	
	
	while(t--){
		cin>>value;
		chk = 0;
		ans = 0;
		
		while(chk < value){
			temp = ans;
			
			oddDigits = 0;
			while(temp > 0){
				dig = temp%10;
				temp = temp/10;
				
				if(dig % 2 == 1){
					oddDigits++;
					break;
				}
			}
			
			
			if(oddDigits == 0)
				chk++;
			ans++;
		}
		
		ans--;
		cout<<ans<<endl;
	}
	return 0;
}

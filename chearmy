#include <iostream>

using namespace std;


int main(){
	int t;
	cin>>t;
	
	

	int num;
	
	int n,copy;
	int prod;
	int sum;

	
	
	
	
	int ans;
	int chk;
	int test;
	int *arr;
	
	int *prev = new int[100000];
	int *current = new int[100000];
	
	int count,i,temp,dig,j;
	
	
	while(t--){
		cin>>ans;
		test = 0;
		chk = 0;
		
		
		while(chk != ans){
			num = test;
			copy = num;
			n = 0;
			while(copy!= 0){
				n++;
				copy = copy/10;
			}
	
	
			arr = new int[n];
	
			for(i = n-1; i >= 0; i--){
				dig = num%10;
				num = num/10;
				arr[i] = dig;
			}
	
	
			sum = 0;
	
			for(i = 1; i <= n; i++){
				current[i-1] = i;
		
				sum += arr[current[i-1]-1];
			}
	
			
			count = n-1;
			for(int x = 2; x <= n; x++){
				for(i = 0; i<=count; i++)
					prev[i] = current[i];
			
				temp = count;
				count = 0;
		
				for(i = 0; i <= temp; i++){
					dig = prev[i]%10;
					for(j = dig+1; j<=n; j++){
						current[count] = prev[i]*10 + j;
						count++;
					}
				}
				count--;
		
				for(j = 0; j<=count; j++){
					copy = current[j];
					prod = 1;
					while(copy!= 0){
						dig =copy%10;
						prod = prod *arr[dig-1];
						copy = copy/10;
					}
			
					sum += prod;
				}
		
				
			}
		
		
		
			if(sum %2 == 0)
				chk++;
			test++;
		}
		test--;
		cout<<test<<endl;
	}
	
	return 0;
}

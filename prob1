#include<iostream>
using namespace std;

int main(){
	
	int t;
	int one,two,three;
	one = 1;
	two = 0;
	three = 0;
	int n,i,j;
	int avg,temp;
	cin>>t;
	
	while(t--){
		avg = 0;
		one = 1;
		two = 0;
		three = 0;
		cin>>n;
		for(i=1; i<=n; i++){
			cin>>temp;
			avg = avg + temp;
			
			if(temp == 2)
				one = 0;
			else if(temp == 5)
				two = 1;
		}
		
		avg = avg / n;
		if(avg >=4)
			three = 1;
		
		if(one == 1 && two == 1 && three == 1)	
			cout<<"Yes"<<endl;
		else
			cout<<"No"<<endl;
	}
		
	return 0;
}

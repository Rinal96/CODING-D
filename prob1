#include<iostream>
using namespace std;

int main(){
	
	int t;
	int num1, num2;
	int dig1;
	int dig2;
	
	int numOf0;
	int numOf1;
	
	int big;
	
	int type1, type2;					//type1 = 0 to 1
									//type2 = 1 to 0
	
	cin>>t;
	while(t--){
		cin>>num1>>num2;
		
		type1 = 0; type2 = 0;
		numOf0 = 0; numOf1 = 0;
		
		while(num1 > 0 || num2 > 0){
			dig1 = num1%10; num1 = num1/10;
			dig2 = num2%10; num2 = num2/10;
			
			if(dig1 == 0){
				numOf0++;
				if(dig2 == 1)
					type1++;
			}
			else{
				numOf1++;
				if(dig2 == 0)
					type2++;
			}
		}
		
		if(numOf0 == 0 || numOf1 == 0)
			cout<<"Unlucky Chef"<<endl;
		else{
			if(type1 > type2)
				big = type1;
			else
				big = type2;
			
			cout<<"Lucky Chef"<<endl;
			cout<<big<<endl;
		}
	}		
			
	
	return 0;
}
